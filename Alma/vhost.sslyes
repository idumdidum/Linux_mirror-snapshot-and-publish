<VirtualHost *:443>
    muutaServerName
    muutaDocumentRoot
    ErrorLog apache2logpath/version_error.log
    CustomLog apache2logpath/version_access.log combined
    SSLEngine On
    SSLCertificateFile fullpathtocerticate
    SSLCertificateKeyFile fullpathtokey
    #SSLCertificateChain
<Directory muutaDoc/>
    Options Indexes FollowSymLinks
    AllowOverride None
    Require all granted
</Directory>
</VirtualHost>
